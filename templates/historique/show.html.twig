{% extends 'base.html.twig' %}

{% block title %}Historique{% endblock %}

{% block body %}
    <h1>Historique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ historique.id }}</td>
            </tr>
            <tr>
                <th>Matricule</th>
                <td>{{ historique.matricule }}</td>
            </tr>
            <tr>
                <th>Debut</th>
                <td>{{ historique.debut ? historique.debut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Depart</th>
                <td>{{ historique.depart ? historique.depart|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_historique_index') }}">back to list</a>

    <a href="{{ path('app_historique_edit', {'id': historique.id}) }}">edit</a>

    {{ include('historique/_delete_form.html.twig') }}
{% endblock %}
